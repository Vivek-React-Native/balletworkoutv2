{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@balletworkout/assets": ["./src/assets"],
      "@balletworkout/components": ["./src/common/components"],
      "@balletworkout/common": ["./src/common"],
      "@balletworkout/container": ["./src/container"],
      "@balletworkout/stories": ["./src/stories"],
      "@balletworkout/utilities": ["./src/utilities"]
    },
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
